{
  "name": "sails-hook-webpack",
  "version": "1.0.4",
  "description": "Webpack asset pipeline hook for Sails",
  "keywords": [
    "sails",
    "sailsjs",
    "hook",
    "sails-hook",
    "webpack",
    "build",
    "asset pipeline"
  ],
  "dependencies": {
    "webpack": "^1.12.14"
  },
  "scripts": {
    "postinstall": "npm run build",
    "prepublish": "npm run build",
    "build": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "test": "mocha"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/teamfa/sails-hook-webpack.git"
  },
  "author": {
    "name": "Mike Diarmid",
    "email": "mike.diarmid@teamfa.com"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-syntax-export-extensions": "^6.5.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "mocha": "^2.3.3",
    "sails": "^0.12.1"
  },
  "sails": {
    "isHook": true
  },
  "license": "MIT"
}
